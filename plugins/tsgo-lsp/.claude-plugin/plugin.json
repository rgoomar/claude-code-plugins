{
  "name": "tsgo-lsp",
  "version": "1.0.0",
  "description": "TypeScript Go LSP",
  "author": {
    "name": "Rishi Goomar"
  },
  "homepage": "https://github.com/rgoomar/claude-code-plugins/plugins/tsgo-lsp",
  "repository": "https://github.com/rgoomar/claude-code-plugins/plugins/tsgo-lsp",
  "license": "MIT",
  "keywords": ["lsp", "typescript", "tsgo"],
  "lspServers": {
    "tsgo": {
      "command": "tsgo",
      "args": ["--lsp", "--stdio"],
      "extensionToLanguage": {
        ".ts": "typescript",
        ".tsx": "typescriptreact",
        ".js": "javascript",
        ".jsx": "javascriptreact",
        ".mts": "typescript",
        ".cts": "typescript",
        ".mjs": "javascript",
        ".cjs": "javascript"
      }
    }
  }
}
